<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.vindoozero.studievolg.MainActivity">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        >

        <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:scrollbars="vertical">

    <TextView
        android:id="@+id/dashboard"
        android:layout_width="357dp"
        android:layout_height="356dp"
        android:layout_marginTop="33dp"
        android:background="@drawable/dashboard"
        android:text="@string/dashboard"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.545" />

    <Button
        android:id="@+id/BtnJaarOverzicht"

        android:layout_width="229dp"
        android:layout_height="53dp"
        android:textSize="16sp"
        android:textColor="#FFFFFF"
        android:text="@string/BtnJaarOverzicht"
        android:background="@drawable/mybutton"
        android:visibility="visible"
        tools:text="Cijfer Overzicht"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        android:layout_marginBottom="24dp"
        android:layout_marginTop="30dp"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintTop_toBottomOf="@+id/dashboard" />

    <ProgressBar
        android:id="@+id/progressBarJaar1"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="324dp"
        android:layout_height="24dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:indeterminate="false"
        android:progressDrawable="@drawable/probar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/dashboard"
        android:layout_marginTop="55dp" />

    <ProgressBar
        android:id="@+id/progressBarJaar2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="324dp"
        android:layout_height="24dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:indeterminate="false"
        android:progressDrawable="@drawable/probar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/progressBarJaar1" />

    <ProgressBar
        android:id="@+id/progressBarJaar3"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="324dp"
        android:layout_height="24dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:indeterminate="false"
        android:progressDrawable="@drawable/probar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/progressBarJaar2" />

    <ProgressBar
        android:id="@+id/progressBarJaar4"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="324dp"
        android:layout_height="24dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:indeterminate="false"
        android:progressDrawable="@drawable/probar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/progressBarJaar3" />

    <ProgressBar
        android:id="@+id/progressBarKeuze"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="324dp"
        android:layout_height="24dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:indeterminate="false"
        android:progressDrawable="@drawable/probar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/progressBarJaar4" />

    <TextView
        android:id="@+id/jaar1"
        android:layout_width="50dp"
        android:layout_height="18dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="58dp"
        android:text="@string/jaar1Bar"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Jaar2"
        android:layout_width="50dp"
        android:layout_height="18dp"
        android:layout_marginBottom="11dp"
        android:text="@string/jaar2Barrrr"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textBar2"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/jaar3"
        android:layout_width="50dp"
        android:layout_height="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/jaar3Bar"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="11dp"
        app:layout_constraintBottom_toTopOf="@+id/progressBarJaar3" />

    <TextView
        android:id="@+id/jaar4"
        android:layout_width="50dp"
        android:layout_height="0dp"
        android:layout_marginBottom="11dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/jaar33baar"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/progressBarJaar4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/Keuzevakken"
        android:layout_width="142dp"
        android:layout_height="19dp"
        android:layout_marginBottom="11dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/Keuzzbaaarr"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/progressBarKeuze"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:text="Keuzevakken" />

    <TextView
        android:id="@+id/textBar1"
        android:layout_width="84dp"
        android:layout_height="25dp"
        android:text="@string/textBar1"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/progressBarJaar1"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/progressBarJaar1"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/jaar1" />

    <TextView
        android:id="@+id/textBar2"
        android:layout_width="84dp"
        android:layout_height="25dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/textBar2"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@+id/progressBarJaar2"
        app:layout_constraintRight_toRightOf="@+id/progressBarJaar2"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/jaar3" />

    <TextView
        android:id="@+id/textBar3"
        android:layout_width="84dp"
        android:layout_height="25dp"
        android:text="@string/textBar3"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/progressBarJaar3"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/progressBarJaar3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/jaar3" />

    <TextView
        android:id="@+id/textBar4"
        android:layout_width="84dp"
        android:layout_height="25dp"
        android:text="@string/textBar1"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/progressBarJaar4"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/progressBarJaar4"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/jaar4" />

    <TextView
        android:id="@+id/keuzeBar"
        android:layout_width="84dp"
        android:layout_height="25dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/textBar1"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@+id/progressBarKeuze"
        app:layout_constraintRight_toRightOf="@+id/progressBarKeuze"
        app:layout_constraintTop_toBottomOf="@+id/Keuzevakken" />

</android.support.constraint.ConstraintLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>
